{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Default": "data source=<server name>;initial catalog=Teta;trusted_connection=true;encrypt=False"
  },
  "Jwt": {
    "Secret": "<your secret>"
  },
  "Amazon": {
    "S3": {
      "AccessKey": "<S3 access key>",
      "SecretKey": "<S3 secret key>",
      "BucketName": "<S3 bucket name>"
    }
  },
  "Stripe": {
    "SecretKey": "<stripe secret key>",
    "PublishableKey": "<stripe publishable key>",
    "WebHookSecret": "<stripe webhook service>",
    "SuccessUrl": "<front end success url>",
    "CancelUrl": "<front end cancel url>",
    "PaymentMethods": [
      "card"
    ]
  },
  "SendGrid": {
    "ApiKey": "<send grid api key>",
    "FromEmail": "notifications@tetatet.network",
    "FromName": "Tetatet App"
  },
  "EmailRestoreLink": "https://tete-a-tete.network/restore",
  "AllowedHosts": "*",
  "AllowedCorsOrigins": [
    "http://localhost:3000",
    "https://tetatet.network",
    "https://tete-a-tete.network"
  ]
}
